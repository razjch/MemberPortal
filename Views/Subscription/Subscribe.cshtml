@model MemberPortal.Models.MemberSubscription

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Subscribe</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class="drugheader">
        @Html.Partial("_partialHeader")
    </div>
    <div class="container">
        <h4>MemberSubscription</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SubscriptionId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SubscriptionId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MemberId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MemberId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SubscriptionDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SubscriptionDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PrescriptionId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PrescriptionId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.RefillOccurrence)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.RefillOccurrence)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MemberLocation)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MemberLocation)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SubscriptionStatus)
            </dt>
            <dd class="col-sm-10">               
                <div class="form-check ">                    
                    @Html.DisplayFor(model => model.SubscriptionStatus)
                </div>
            </dd>
            <br>
            <br>
            <br>
            <dt class="col-sm-2">
                <a style="background-color: lightgray; text-align: center;" class="nav-link text-dark " asp-area="" asp-controller="Subscription" asp-action="GetAllPrescription">Back To List</a>
            </dt>

        </dl>
    </div>
    <div class="drugfooter">
        @Html.Partial("_partialFooter")
    </div>
</body>

<script>
    if ('@ViewBag.MessageSubscribe'!='')
        alert('@ViewBag.MessageSubscribe');
    if ('@ViewBag.MessageUnsubscribe'!='')
        alert('@ViewBag.MessageUnsubscribe');
</script>
</html>
